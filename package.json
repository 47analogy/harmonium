  "devDependencies": {
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/exec": "^3.3.5",
    "@semantic-release/git": "^7.0.7",
    "@semantic-release/npm": "^5.1.3",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "semantic-release": "^15.1.7"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/exec",
        "publishCmd": "npx lerna version ${nextRelease.version} --no-git-tag-version"
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "lerna.json",
          "packages/*/package.json"
        ],
        "message": "chore(release): Bump version to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}
